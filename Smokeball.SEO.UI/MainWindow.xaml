<Window x:Class="Smokeball.SEO.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smokeball.SEO"
        mc:Ignorable="d" FontSize="14"
        Title="MainWindow" Height="400" Width="800">
    <Grid>
        <TextBox x:Name="UrlField" HorizontalAlignment="Left" Margin="127,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="635" Text="smokeball.com.au"/>
        <Label Content="URL" HorizontalAlignment="Left" Margin="37,28,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="KeywordsField" HorizontalAlignment="Left" Margin="127,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="635" Text="conveyancing software"/>
        <Label Content="Keywords" HorizontalAlignment="Left" Margin="37,121,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Search" Content="Search" HorizontalAlignment="Center" Margin="0,203,0,0" VerticalAlignment="Top" Click="Search_Button_Click" Height="35" Width="144"/>
        <TextBlock x:Name="ShowResult" HorizontalAlignment="Center" Margin="0,279,0,0" VerticalAlignment="Top" Width="32" FontSize="28"/>
    </Grid>
</Window>
