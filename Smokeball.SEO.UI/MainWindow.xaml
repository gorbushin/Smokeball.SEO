<Window x:Class="Smokeball.SEO.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Smokeball.SEO.UI"
        mc:Ignorable="d" FontSize="14"
        Title="MainWindow" Height="336" Width="695">
    <Grid>
        <TextBox x:Name="UrlField" HorizontalAlignment="Left" Margin="154,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="505" Text="smokeball.com.au"/>
        <Label Content="URL to check" HorizontalAlignment="Left" Margin="14,83,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="KeywordsField" HorizontalAlignment="Left" Margin="154,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="505" Text="conveyancing software"/>
        <Label Content="Search for keywords" HorizontalAlignment="Left" Margin="14,24,0,0" VerticalAlignment="Top"/>
        <Button x:Name="Search" Content="Search" HorizontalAlignment="Center" VerticalAlignment="Top" Click="Search_Button_Click" Height="35" Width="144" Margin="0,156,0,0"/>
        <TextBlock x:Name="ShowResult" TextAlignment="Center" HorizontalAlignment="Center" Margin="0,241,0,0" VerticalAlignment="Top" Width="667" FontSize="22"/>
    </Grid>
</Window>
